<div class="searchbar">
  <form
    action="/search"
    method="GET"
    style="display: flex; align-items: center"
  >
    <input
      type="text"
      placeholder="Search.."
      name="search"
      style="padding: 10px; border-radius: 20px; width: 450px"
    />
    <button
      type="submit"
      style="
        border: none;
        cursor: pointer;
        appearance: none;
        background-color: inherit;
      "
    >
      <img src="../images/search.png" style="width: 50px; height: 50px" />
    </button>
  </form>
</div>
